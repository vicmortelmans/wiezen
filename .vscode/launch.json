    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to process",
            "restart": true,
            "port": 9229
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch console.js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/console.js",
            "console": "integratedTerminal" 
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch ai_players",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/ai_players.js",
            "console": "integratedTerminal" 
        }
    ]
}